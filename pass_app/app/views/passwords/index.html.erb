<div class="header">
    <h1>How Strong is my Password</h1>     
</div>
<%= link_to 'Test Your Pasword', new_password_path %>
<table>
  <thead>
    <tr>
      <th>Words</th>
      <th colspan="3">This will leave</th>
    </tr>
  </thead>

  <tbody>
    <% @passwords.each do |password| %>
      <tr>
        <td><%= password.pass %></td>
        <td><%= link_to 'Show', password %></td>
        <td><%= link_to 'Edit', edit_password_path(password) %></td>
        <td><%= link_to 'Destroy', password, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

